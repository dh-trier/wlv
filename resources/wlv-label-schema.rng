<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <ref name="wlv" />
  </start>
  <define name="wlv">
    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The root element in a label description using the Wine Label Vocabulary.</a:documentation>
    <element>
      <name ns="">wlv</name>
      <ref name="labelID" />
      <optional>
        <ref name="labelType" />
      </optional>
      <ref name="metadata" />
      <choice>
        <ref name="labelGroup" />
        <ref name="label" />
      </choice>
      <optional>
        <ref name="comments" />
      </optional>
      <optional>
        <ref name="provenance" />
      </optional>
      <optional>
        <ref name="relations" />
      </optional>
    </element>
  </define>
  <define name="ID">
    <attribute>
      <name ns="">ID</name>
        <text/>
    </attribute>
  </define>
  <define name="labelID">
    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A unique identifier for the label.</a:documentation>
    <attribute>
      <name ns="">labelID</name>
      <text />
    </attribute>
  </define>
