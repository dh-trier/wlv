<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="wlv">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="metadata"/>
        <xs:choice>
          <xs:element ref="labelGroup"/>
          <xs:element ref="label"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="comments"/>
        <xs:element minOccurs="0" ref="provenance"/>
        <xs:element minOccurs="0" ref="relations"/>
      </xs:sequence>
      <xs:attributeGroup ref="labelID"/>
      <xs:attribute name="labelType"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="ID">
    <xs:attribute name="ID" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="labelID">
    <xs:annotation>
      <xs:documentation>A unique identifier for the label.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="labelID" use="required"/>
  </xs:attributeGroup>
  <!--
    ========================================
    ======        METADATEN            =====
    ========================================  
  -->
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="collection"/>
        <xs:element ref="curation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="licence"/>
        <xs:element minOccurs="0" ref="collectionContext"/>
        <xs:element minOccurs="0" ref="comments"/>
      </xs:sequence>
      <xs:attribute name="uri"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="collection">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="collectionID"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="collectionID">
    <xs:annotation>
      <xs:documentation>A unique identifier for the wine label collection.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="collectionID" use="required"/>
  </xs:attributeGroup>
  <xs:element name="curation">
    <xs:complexType>
      <xs:attributeGroup ref="curatorID"/>
      <xs:attributeGroup ref="curationDate"/>
      <xs:attribute name="curationUpdate">
        <xs:simpleType>
          <xs:union memberTypes="xs:int xs:date"/>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="curatorID">
    <xs:annotation>
      <xs:documentation>A unique identifier for the person that has curated the wine label description.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="curatorID" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="curationDate">
    <xs:annotation>
      <xs:documentation>The date (or year) when the wine label description was created.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="curationDate" use="required">
      <xs:simpleType>
        <xs:union memberTypes="xs:int xs:date"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="curationUpdate">
    <xs:annotation>
      <xs:documentation>The date (or year) when the wine label description was last updated.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="curationUpdate" use="required">
      <xs:simpleType>
        <xs:union memberTypes="xs:int xs:date"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="collectionContext">
    <xs:complexType mixed="true">
      <xs:attribute name="pageID"/>
      <xs:attribute name="scanID"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="pageID">
    <xs:attribute name="pageID" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="scanID">
    <xs:attribute name="scanID" use="required"/>
  </xs:attributeGroup>
  <xs:element name="licence">
    <xs:complexType mixed="true">
      <xs:attribute name="uri"/>
      <xs:attribute name="licenceAbbr"/>
      <xs:attributeGroup ref="licenceScope"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="licenceAbbr">
    <xs:annotation>
      <xs:documentation>Abbreviation for the licence.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="licenceAbbr" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="licenceScope">
    <xs:annotation>
      <xs:documentation>The scope of the licence: The part or aspect of the label or label description to which the licence apply.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="licenceScope" use="required"/>
  </xs:attributeGroup>
  <!--
    ========================================
    ======        LABEL                =====
    ========================================  
  -->
  <xs:element name="labelGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="comments"/>
        <xs:element maxOccurs="unbounded" ref="label"/>
        <xs:element minOccurs="0" ref="provenance"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="labelType">
    <xs:annotation>
      <xs:documentation>This describes the label primarily with regard to the agent issuing it and the geographical scope.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="labelType" use="required"/>
  </xs:attributeGroup>
  <xs:element name="comments">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="comment"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="comment">
    <xs:complexType mixed="true">
      <xs:attribute name="ID"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="label">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="physical"/>
        <xs:element ref="visual"/>
        <xs:element ref="textual"/>
        <xs:element minOccurs="0" ref="relations"/>
      </xs:sequence>
      <xs:attribute name="labelPosition">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="front"/>
            <xs:enumeration value="back"/>
            <xs:enumeration value="neck"/>
            <xs:enumeration value="wraparound"/>
            <xs:enumeration value="band"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="labelStatus">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="physical label present"/>
            <xs:enumeration value="print copy present"/>
            <xs:enumeration value="digital copy present"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="labelPosition">
    <xs:annotation>
      <xs:documentation>The position of the label on the bottle, used in particular as a way to disinguish the different labels in a label group.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="labelPosition" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="front"/>
          <xs:enumeration value="back"/>
          <xs:enumeration value="neck"/>
          <xs:enumeration value="wraparound"/>
          <xs:enumeration value="band"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="labelStatus">
    <xs:annotation>
      <xs:documentation>The status of the label: physically present in the collection, or only as digital copy.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="labelStatus" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="physical label present"/>
          <xs:enumeration value="print copy present"/>
          <xs:enumeration value="digital copy present"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    ========================================
    ======        PHYSICAL             =====
    ========================================  
  -->
  <xs:element name="physical">
    <xs:complexType>
      <xs:attributeGroup ref="shape"/>
      <xs:attribute name="sizeH" type="xs:int"/>
      <xs:attribute name="sizeV" type="xs:int"/>
      <xs:attribute name="material">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="paper"/>
            <xs:enumeration value="plastic"/>
            <xs:enumeration value="metal"/>
            <xs:enumeration value="mixed"/>
            <xs:enumeration value="undetermined"/>
            <xs:enumeration value="other"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="printingTechnique">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="lithograph"/>
            <xs:enumeration value="offset"/>
            <xs:enumeration value="moving-type"/>
            <xs:enumeration value="litho+type"/>
            <xs:enumeration value="mixed"/>
            <xs:enumeration value="undetermined"/>
            <xs:enumeration value="other"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="shape">
    <xs:annotation>
      <xs:documentation>Information regarding the shape of the physical label.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="shape" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="square"/>
          <xs:enumeration value="circle"/>
          <xs:enumeration value="rectangle"/>
          <xs:enumeration value="oval"/>
          <xs:enumeration value="diamond"/>
          <xs:enumeration value="trapezoid"/>
          <xs:enumeration value="octogon"/>
          <xs:enumeration value="circular"/>
          <xs:enumeration value="ellipsoid"/>
          <xs:enumeration value="polygon"/>
          <xs:enumeration value="free-form"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="sizeH">
    <xs:annotation>
      <xs:documentation>The height of the label measured in millimeters. (This attribute is mandatory, but can also take the value "tbc" if the size data is not available.)</xs:documentation>
    </xs:annotation>
    <xs:attribute name="sizeH" use="required" type="xs:int"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="sizeV">
    <xs:annotation>
      <xs:documentation>The width of the label measured in millimeters. (This attribute is mandatory, but can also take the value "tbc" if the size data is not available.)</xs:documentation>
    </xs:annotation>
    <xs:attribute name="sizeV" use="required" type="xs:int"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="material">
    <xs:annotation>
      <xs:documentation>The material from which the label is made (in most cases, this is paper).</xs:documentation>
    </xs:annotation>
    <xs:attribute name="material" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="paper"/>
          <xs:enumeration value="plastic"/>
          <xs:enumeration value="metal"/>
          <xs:enumeration value="mixed"/>
          <xs:enumeration value="undetermined"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="printingTechnique">
    <xs:annotation>
      <xs:documentation>The technical printing process used for printing the visual and/or textual information onto the base material.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="printingTechnique" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="lithograph"/>
          <xs:enumeration value="offset"/>
          <xs:enumeration value="moving-type"/>
          <xs:enumeration value="litho+type"/>
          <xs:enumeration value="mixed"/>
          <xs:enumeration value="undetermined"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    ========================================
    ======        VISUAL               =====
    ========================================  
  -->
  <xs:element name="visual">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="frame"/>
        <xs:element ref="background"/>
        <xs:element ref="figure"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="frame">
    <xs:complexType>
      <xs:attribute name="ID"/>
      <xs:attribute name="position" type="xs:int"/>
      <xs:attribute name="frameType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="outer"/>
            <xs:enumeration value="inner"/>
            <xs:enumeration value="other"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="frameStyle">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="line(s)"/>
            <xs:enumeration value="band(s)"/>
            <xs:enumeration value="floral"/>
            <xs:enumeration value="pattern"/>
            <xs:enumeration value="ornamental line(s)"/>
            <xs:enumeration value="ornamental band(s)"/>
            <xs:enumeration value="other"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="frameColor"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="frameStyle">
    <xs:annotation>
      <xs:documentation>The visual style of the frame.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="frameStyle" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="line(s)"/>
          <xs:enumeration value="band(s)"/>
          <xs:enumeration value="floral"/>
          <xs:enumeration value="pattern"/>
          <xs:enumeration value="ornamental line(s)"/>
          <xs:enumeration value="ornamental band(s)"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="frameType">
    <xs:annotation>
      <xs:documentation>The type of frame, defined based on its position relative to other elements of the label. The value 'outer' means all other elements are within the frame. The value 'inner' means some of the other elements of the label are within, others are outside the frame.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="frameType" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="outer"/>
          <xs:enumeration value="inner"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="frameColor">
    <xs:annotation>
      <xs:documentation>The color of the frame.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="frameColor" use="required"/>
  </xs:attributeGroup>
  <xs:element name="background">
    <xs:complexType>
      <xs:attributeGroup ref="backgroundColor"/>
      <xs:attribute name="ID"/>
      <xs:attribute name="backgroundStyle">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="pattern"/>
            <xs:enumeration value="color"/>
            <xs:enumeration value="none"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="backgroundStyle">
    <xs:annotation>
      <xs:documentation>The kind of background, whether a pattern, a flat color plane, or an absence of any pattern or color.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="backgroundStyle" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="pattern"/>
          <xs:enumeration value="color"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="backgroundColor">
    <xs:annotation>
      <xs:documentation>The dominant color tone, whether it is strong or faint.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="backgroundColor" use="required"/>
  </xs:attributeGroup>
  <xs:element name="figure">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="figureItem"/>
      </xs:sequence>
      <xs:attribute name="figureType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="symbolic">
              <xs:annotation>
                <xs:documentation>(figure type: symbolic) Used for any figure that uses realistic elements in a stylized manner.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="realistic">
              <xs:annotation>
                <xs:documentation>(figure type: realistic) Used for any figure that represents elements from the real world in a recognizable, realistic manner.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="abstract">
              <xs:annotation>
                <xs:documentation>(figure type: abstract) Used for any figure that predominantly uses abstract shapes, forms and lines without representing a clearly-recognizable element from the real world.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="seal">
              <xs:annotation>
                <xs:documentation>(figure type: seal) Used for any representation of a seal.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="coat-of-arms">
              <xs:annotation>
                <xs:documentation>(figure type: coat-of-arms) Used for any representation of a coat of arms.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="other">
              <xs:annotation>
                <xs:documentation>(figure type: other) To be used sparingly for any kind of figure that eludes classification.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="position" type="xs:int"/>
      <xs:attribute name="ID"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="figureType">
    <xs:annotation>
      <xs:documentation>(figure type) Allows a simple classification of figures into common types.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="figureType" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="symbolic">
            <xs:annotation>
              <xs:documentation>(figure type: symbolic) Used for any figure that uses realistic elements in a stylized manner.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="realistic">
            <xs:annotation>
              <xs:documentation>(figure type: realistic) Used for any figure that represents elements from the real world in a recognizable, realistic manner.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="abstract">
            <xs:annotation>
              <xs:documentation>(figure type: abstract) Used for any figure that predominantly uses abstract shapes, forms and lines without representing a clearly-recognizable element from the real world.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="seal">
            <xs:annotation>
              <xs:documentation>(figure type: seal) Used for any representation of a seal.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="coat-of-arms">
            <xs:annotation>
              <xs:documentation>(figure type: coat-of-arms) Used for any representation of a coat of arms.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="other">
            <xs:annotation>
              <xs:documentation>(figure type: other) To be used sparingly for any kind of figure that eludes classification.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="position">
    <xs:annotation>
      <xs:documentation>(position) The position of a visual or textual element on the label surface, in a grid of nine quadrants. The quadrants are numbered like on the number-pad of a phone, starting at the top left with 1 and ending at the bottom right with 9. Any quadrant that the figure covers to a significant extent is included in the position description. </xs:documentation>
    </xs:annotation>
    <xs:attribute name="position" use="required" type="xs:int"/>
  </xs:attributeGroup>
  <xs:element name="figureItem">
    <xs:complexType>
      <xs:attributeGroup ref="norm"/>
      <xs:attribute name="ID"/>
    </xs:complexType>
  </xs:element>
  <!--
    ===========================
    ===== FONT ATTRIBUTES =====
    ===========================
  -->
  <xs:attributeGroup name="fontAttributes">
    <xs:attribute name="fontType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="Fraktur"/>
          <xs:enumeration value="Antiqua-Sans"/>
          <xs:enumeration value="Antiqua-Serif"/>
          <xs:enumeration value="Cursive"/>
          <xs:enumeration value="Altdeutsch"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="fontManner">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="printed"/>
          <xs:enumeration value="handwritten"/>
          <xs:enumeration value="stamped"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="fontStyle">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="recte"/>
          <xs:enumeration value="italics"/>
          <xs:enumeration value="small-caps"/>
          <xs:enumeration value="all-caps"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="fontColor"/>
    <xs:attribute name="fontInitials">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="color:red"/>
          <xs:enumeration value="color:black"/>
          <xs:enumeration value="color:other"/>
          <xs:enumeration value="size:larger"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="fontSize">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="larger"/>
          <xs:enumeration value="normal"/>
          <xs:enumeration value="smaller"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    =====================================
    ======         TEXTUAL          =====
    =====================================
  -->
  <xs:element name="textual">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="wineMillesime"/>
          <xs:element ref="wineName"/>
        </xs:choice>
        <xs:element ref="wineGrapes"/>
        <xs:element ref="wineTaste"/>
        <xs:element ref="wineAging"/>
        <xs:element ref="wineOther"/>
        <xs:element ref="qualityGrapes"/>
        <xs:element ref="qualityLevel"/>
        <xs:element ref="qualityAward"/>
        <xs:element ref="qualityLabel"/>
        <xs:element ref="qualityProduction"/>
        <xs:element ref="qualityHistorical"/>
        <xs:element ref="agent"/>
        <xs:element ref="location"/>
        <xs:element ref="alcohol"/>
        <xs:element ref="volume"/>
        <xs:element ref="controlNumber"/>
        <xs:element ref="barrelNumber"/>
        <xs:element ref="labelNumber"/>
        <xs:element ref="otherText"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="wineMillesime">
    <xs:complexType mixed="true">
      <xs:attribute name="numNorm" type="xs:float"/>
      <xs:attribute name="ID"/>
      <xs:attribute name="position" type="xs:int"/>
      <xs:attributeGroup ref="fontAttributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="wineName">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="agent"/>
        <xs:element ref="location"/>
        <xs:element ref="wineMillesime"/>
        <xs:element ref="qualityGrapes"/>
        <xs:element ref="qualityLabel"/>
        <xs:element ref="qualityLevel"/>
        <xs:element ref="wineGrapes"/>
      </xs:choice>
      <xs:attribute name="wineNameType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="country / Land"/>
            <xs:enumeration value="region / Gebiet"/>
            <xs:enumeration value="area / Bereich"/>
            <xs:enumeration value="locality / Ortsname"/>
            <xs:enumeration value="locality / Leitgemeinde"/>
            <xs:enumeration value="winery / Weingut"/>
            <xs:enumeration value="vineyard / historischer Lagenname"/>
            <xs:enumeration value="vineyard / Lage"/>
            <xs:enumeration value="vineyard / Großlage"/>
            <xs:enumeration value="vineyard / Einzellage"/>
            <xs:enumeration value="variety / Rebsorte"/>
            <xs:enumeration value="composite"/>
            <xs:enumeration value="other"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="norm">
        <xs:simpleType>
